{
  "name": "movies-selector-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:report": "yarn lint -f json -o ./coverage/eslint.report.json",
    "format": "prettier --write **/*.{js,jsx,ts,tsx,json} && prettier --write *.{js,jsx,ts,tsx,json}",
    "test": "jest --passWithNoTests",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn test --coverage",
    "test:report": "yarn test:coverage --testResultsProcessor jest-sonar-reporter"
  },
  "dependencies": {
    "@expo-google-fonts/allan": "^0.2.2",
    "@expo-google-fonts/bangers": "^0.2.2",
    "@expo-google-fonts/roboto": "^0.2.0",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.2",
    "axios": "^0.27.2",
    "expo": "^45.0.0",
    "expo-app-loading": "~2.0.0",
    "expo-file-system": "~14.0.0",
    "expo-font": "~10.1.0",
    "expo-status-bar": "~1.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.69.0",
    "react-native-paper": "^4.12.1",
    "react-native-safe-area-context": "4.3.1",
    "react-native-screens": "~3.13.1",
    "react-native-svg": "12.3.0",
    "react-native-vector-icons": "^9.0.0",
    "react-native-web": "0.18.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@faker-js/faker": "^7.3.0",
    "@testing-library/jest-native": "^4.0.5",
    "@testing-library/react-native": "^9.1.0",
    "@types/jest": "^28.1.3",
    "@types/react": "~18.0.14",
    "@types/react-native": "~0.68.0",
    "@types/react-native-vector-icons": "^6.4.10",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "jest-expo": "^45.0.1",
    "jest-sonar-reporter": "^2.0.0",
    "msw": "^0.42.3",
    "prettier": "^2.7.1",
    "react-native-dotenv": "^3.3.1",
    "react-test-renderer": "18.2.0",
    "ts-node": "^10.8.1",
    "typescript": "~4.7.4"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/**/*.{ts,tsx}": [
      "eslint --ext .tsx --ext .ts ./src"
    ],
    "./src/**": [
      "prettier --check ./src"
    ]
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-reporter.xml",
    "indent": 4
  },
  "globals": {
    "ts-jest": {
      "tsConfig": "./tsconfig.json"
    }
  }
}
