{
  "name": "movies-selector-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint:ts": "eslint --ext .js,.jsx,.ts,.tsx,.json src/",
    "lint:ts-fix": "eslint --ext .js,.jsx,.ts,.tsx,.json src/ --fix",
    "lint:ts-report": "yarn lint:ts -f json ./scripts/sonarqube_eslint_formatter.js -o ./coverage/eslint.report.json",
    "lint:style": "stylelint **/styles/**/*.ts",
    "lint:style-report": "stylelint **/styles/**/*.ts -o ./coverage/stylelint.report.json",
    "lint:all": "yarn lint:ts & yarn lint:style",
    "lint:all-report": "yarn lint:ts-report & yarn lint:style-report",
    "format": "prettier --write {src,setup-test,scripts,__{tests,mocks}__}/**/*.{js,jsx,ts,tsx,json} *.{js,ts}",
    "test": "jest --passWithNoTests",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn test --coverage",
    "validate-publish-version": "ts-node ./scripts/validatePublishVersion.ts",
    "rewrite-status-badges": "yarn ts-node ./scripts/changeBranchInTheReadme.ts",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@expo-google-fonts/allan": "^0.2.2",
    "@expo-google-fonts/bangers": "^0.2.2",
    "@expo-google-fonts/roboto": "^0.2.0",
    "@react-navigation/native": "^6.0.11",
    "@react-navigation/native-stack": "^6.7.0",
    "axios": "^0.27.2",
    "expo": "^45.0.0",
    "expo-app-loading": "~2.0.0",
    "expo-file-system": "~14.0.0",
    "expo-font": "~10.1.0",
    "expo-status-bar": "~1.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.69.3",
    "react-native-paper": "^4.12.4",
    "react-native-safe-area-context": "4.3.1",
    "react-native-screens": "~3.15.0",
    "react-native-svg": "12.4.3",
    "react-native-vector-icons": "^9.0.0",
    "react-native-web": "0.18.7",
    "realm": "^10.19.5"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@faker-js/faker": "^7.3.0",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@testing-library/jest-native": "^4.0.5",
    "@testing-library/react-native": "^11.0.0",
    "@types/git-rev-sync": "^2.0.0",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.2",
    "@types/react": "~18.0.15",
    "@types/react-native": "~0.69.3",
    "@types/react-native-vector-icons": "^6.4.11",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "git-rev-sync": "^3.0.2",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-expo": "^45.0.1",
    "jest-junit": "^14.0.0",
    "jest-sonar": "^0.2.12",
    "msw": "^0.44.2",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.7.1",
    "react-native-dotenv": "^3.3.1",
    "react-test-renderer": "18.2.0",
    "stylelint": "^14.9.1",
    "stylelint-config-recommended": "^8.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^5.0.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "~4.7.4"
  },
  "private": true,
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/**/*.*": [
      "yarn format"
    ]
  },
  "globals": {
    "ts-jest": {
      "tsConfig": "./tsconfig.json"
    }
  }
}
